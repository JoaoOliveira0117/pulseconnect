{
  "name": "pulseconnect-api",
  "type": "module",
  "version": "1.0.0",
  "description": "api for pulseconnect website",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.js --legacy-watch",
    "start": "node src/index.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migration-generate": "sequelize-cli migration:generate --name",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up --build",
    "lint": "eslint --ext .js ./"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.33.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.4.9",
    "eslint": "^8.49.0",
    "sequelize-cli": "^6.6.1"
  }
}
