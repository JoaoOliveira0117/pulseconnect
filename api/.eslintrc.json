{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
		"eslint:recommended",
		"plugin:prettier/recommended"
	],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
	"plugins": ["prettier"],
	"root": true,
  "rules": {
		"ident": 0,
		"quotes": ["warn", "single"],
		"semi": 0,
		"no-multi-spaces": ["error"]
	}
}